package com.javalec.base;

public class IF_03 {

	public static void main(String[] args) {
		int i = 10;
		int j = 20;
		
		if(i>j) {
			System.out.println("i는 j보다" + (i-j) + "만큼 큽니");
		}else {
			System.out.println("j는 i보다" + (i-j) + "만큼 큽니");
		}
		
		System.out.println("계산이 완료되었씁니");

	}//main

}//IF_03
