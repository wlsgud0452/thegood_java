package com.javalec.base;

public class Main {

	public static void main(String[] args) {
		// 조건문
		int i =10;
		int j =20;
		int k =10;
		
		if(i > j) {
			System.out.println("i가 j보다 큽니다.");
		}
		if( i < j) {
			System.out.println("i가 j보다 작습니다.");
		}
		
		System.out.println(">>> End <<<");
	}

}
